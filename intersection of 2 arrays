class Solution {
public:
    vector<int> intersection(vector<int>& nums1, vector<int>& nums2) {
        vector<int> k;
        for (int i = 0; i < nums1.size(); i++) {
    int count = 0;
    for (int j = 0; j < nums2.size(); j++) {
        if (nums1[i] == nums2[j]) {
            count++;
        }
    }
    if (count >= 1 && find(k.begin(), k.end(), nums1[i]) == k.end()) {
        k.push_back(nums1[i]);
    }
}

 return k;
    }   
};
