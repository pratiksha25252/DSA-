class Solution {
public:
    vector<int> findMissingElements(vector<int>& nums) {
        sort(nums.begin(), nums.end());
       vector<int> missing;
       for(int i=1;i<nums.size();i++){
      int  diff=nums[i]-nums[i-1];
        
        
       if(diff>1){
       for(int j=nums[i-1]+1;j<nums[i];j++){
        missing.push_back(j);
       }
       }
       }
       return missing;
    }
};
       
